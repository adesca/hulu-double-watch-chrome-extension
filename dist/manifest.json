{
  "manifest_version": 2,
  "name": "hulu-simulwatch",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/fancy-d.jpg"
  },

  "content_scripts": [
    {
      "matches": ["*://*.hulu.com/*"],
      "js": ["borderify.js"]
    }
  ]
}
